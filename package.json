{
  "name": "vite-react-admin-main",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix  \"./src/**/*.{tsx,jsx,js,ts}\"",
    "lint:prettier": "prettier --write --loglevel warn  \"src/**/*.{js,ts,json,tsx,jsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:all": "npm run lint:eslint && npm run lint:prettier",
    "prepare": "husky",
    "lint:lint-staged": "lint-staged",
    "cm": "git add -A && cz && git push"
  },
  "dependencies": {
    "@voerkai18n/vite": "^2.1.10",
    "antd": "^5.21.1",
    "eslint": "^9.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-restart": "^0.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "rollup-plugin-external-globals": "^0.12.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass-embedded": "^1.79.3",
    "stylelint": "^16.9.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^4.7.4",
    "typescript-eslint": "^8.0.1",
    "unplugin-antd-resolver": "^0.0.2",
    "unplugin-auto-import": "^0.18.3",
    "vite": "^5.4.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.8.7"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}